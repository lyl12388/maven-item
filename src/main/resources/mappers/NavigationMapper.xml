<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vip.items.mapper.NavigationMapper">
  <resultMap id="nav_result" type="Navigation">
    <id property="navId" column="nav_id"></id>
    <result property="navName" column="nav_name"></result>
    <result property="isDelete" column="is_delete"></result>
  </resultMap>
  <select id="findAll" resultMap="nav_result">
    select nav_id,nav_name
    from navigation n
    where is_delete = 0;
  </select>
</mapper>