<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.vip.items.mapper.ProjectMapper">
    <resultMap id="projest_result" type="Project">
        <id property="proid" column="proid"></id>
        <result property="proName" column="pro_name"></result>
        <result property="proCom" column="pro_com"></result>
        <result property="money" column="money"></result>
        <result property="proRequest" column="pro_request"></result>
        <result property="proLocation" column="pro_location"></result>
        <result property="tel" column="tel"></result>
        <result property="time" column="time"></result>
    </resultMap>
    <sql id="project_datail">
    proid,pro_name,pro_com,money,
    pro_request,pro_location,tel,time
    </sql>
  <select id="findByproId" resultMap="projest_result">
    select
      <include refid="project_datail" />
    from project
    where proid = #{proId}  and is_delete = 0
  </select>
</mapper>